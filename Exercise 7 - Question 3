import pandas
import numpy
InFile3=pandas.read_csv('data.txt')
north=[]
east=[]
south=[]
west=[]
for Line in InFile3:
		if 'north' in Line:
			north.append([1:])
		elif 'east' in Line:
		    east.append(Line[1:])
		elif 'south' in Line:
		    south.append(Line[1:])
		elif 'west' in Line:
		    west.append(Line[1:])

data3 = pandas.DataFrame(list(zip(north, east, south, 
west)),columns=['north','east','south','west'])

#to close the file
InFile3.close()
